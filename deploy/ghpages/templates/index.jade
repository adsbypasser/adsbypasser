doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible' content='chrome=1')
    title= locals.title

    link(rel='stylesheet' href='css/index.css')
    meta(name='viewport' content='width=device-width, initial-scale=1, user-scalable=no')
  body
    .wrapper
      header
        p.view
          a(href='https://github.com/adsbypasser/adsbypasser') View the Project on GitHub
            small adsbypasser/adsbypasser

        .links
          a(href='https://adsbypasser.github.io/releases/adsbypasser.user.js') #[strong Install Full Version]
          a(href='https://adsbypasser.github.io/releases/adsbypasserlite.user.js') #[strong Install Lite Version]

        .links
          a(href='https://adsbypasser.github.io/configure.html') #[strong Configure Settings]

        .links
          a(href='https://github.com/adsbypasser/adsbypasser/issues') #[strong Report Bug / Request Feature]
      section
        != contents['summary.md'].html
      footer
        p
          |This project is maintained by&nbsp;
          a(href='https://github.com/legnaleurc') legnaleurc
        p
          small Hosted on GitHub Pages
            br
            |&mdash; Theme by&nbsp;
            a(href='https://github.com/orderedlist') orderedlist
            br
            |&mdash; powered by&nbsp;
            a(href='https://github.com/jnordberg/wintersmith') Wintersmith

    script(src='js/scale.fix.js')
